{#
/**
 * @file
 * Default simple view template to display a list of rows.
 *
 * - $title : The title of this group of rows.  May be empty.
 * - $options['type'] will either be ul or ol.
 * @ingroup views_templates
 */#}
<section class="band">
  {{ wrapper_prefix }}
    <section class="">
      {{ list_type_prefix }}
      {# PHP: foreach ($rows as $id => $row): #}
        <li itemscope="" itemtype="http://schema.org/ImageGallery" class="{{ classes_array }}">{{ row }}</li>
      {% endfor %}
      {{ list_type_suffix }}
    </section>
   {{ wrapper_suffix }}
</section>
