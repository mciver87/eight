/**
 * Implements hook_theme().
 */
function nc_fields_theme($existing, $type, $theme, $path) {
  $items = array();
  $path = drupal_get_path('module', 'nc_fields');
  $items['nc_term_label'] = array(
    'arguments' => array(
      'term' => NULL,
      'type' => 'simple',
      'language' => LANGUAGE_NONE,
    ),
    'file' => 'nc_fields.theme.inc',
    'path' => $path,
  );
  $items['nc_file_detail'] = array(
    'arguments' => array(
      'file' => NULL,
      'node' => NULL,
      'language' => LANGUAGE_NONE,
    ),
    'file' => 'nc_fields.theme.inc',
    'path' => $path,
  );
  $items['nc_date'] = array(
    'arguments' => array(
      'date' => time(),
      'label' => t('Release Date'),
      'language' => LANGUAGE_NONE,
    ),
    'file' => 'nc_fields.theme.inc',
    'path' => $path,
  );
  return $items;
}

