/**
 * Implements hook_field_formatter_view().
 */
function nc_fields_field_formatter_view($entity_type, $entity, $field, $instance, $langcode, $items, $display) {
  $element = array();
  // ABT#620 - Noticed that the group parent displays even if there
  // are no items selected (empty array). This should fix that.
  if (!$items || !isset($items)) {
    return;
  }

  switch ($display['type']) {
    case 'nc_fields_term_formatter_simple':
      foreach ($items as $delta => $item) {
        $term = taxonomy_term_load($item['tid']);
        $element[$delta] = theme('nc_term_label', array(
          'term' => $term,
          'type' => 'simple',
          'language' => $langcode)
        );
      }
      break;

    case 'nc_fields_file_formatter_detail':
      foreach ($items as $delta => $item) {
        $element[$delta] = theme('nc_file_detail', array(
          'file' => $item,
          'node' => $entity,
          'language' => $langcode)
        );
      }
      break;

    case 'nc_fields_date_formatter':
      foreach ($items as $delta => $item) {
        $element[$delta] = theme('nc_date', array(
          'date' => $item,
          'label' => $instance['label'],
          'language' => $langcode)
        );
      }
      break;

  }
  $element['#attached']['css'] = array(
    drupal_get_path('module', 'nc_fields') . '/nc_fields.css',
  );

  return $element;
}

/**
 * Implements hook_form_system_theme_settings_alter().
 *
 * This is being used to add a new settings to the theme/appearance
 * where administrative users can toggle between the default
 * and alternate document icons.
 *
 * See: http://www.digitalstyle.nc.gov/section-1.html#section-1.4.2
 */
function nc_fields_form_alter(&$form, $form_state) {
  // Ensure we're in the theme settings form.
  if (isset($form['var']) &&
    isset($form['var']['#value']) &&
    $form['var']['#value'] == 'theme_nc_base_theme_settings'
  ) {
    // The setting is stored with the theme.
    $theme = variable_get('theme_default', 'nc_base_theme');
    // Add the Document icon dropdown.
    $form['nc_base_theme_settings']['document_type_icon_fieldset'] = array(
      '#type' => 'fieldset',
      '#title' => t('Document Type Icons'),
      '#description' => t('Document Type Icons are the icons displayed in association with various document types (i.e. doc, xls, image, etc)'),
    );
    $form['nc_base_theme_settings']['document_type_icon_fieldset']['document_type_icon_style'] = array(
      '#type' => 'select',
      '#title' => t('Document Type Icons Style'),
      '#default_value' => theme_get_setting('document_type_icon_style', $theme),
      '#description' => t('Select an icon style for use with document types.'),
      '#options' => array(
        '' => t("Default"),
        'alt' => t("Alt"),
      ),
    );
  }
}

/**
 * Get the style modifier associated with the theme settings.
 *
 * @return string
 *   Return the icon class modifier (i.e. -alt).
 */
function nc_fields_get_document_type_icon_style() {
  // This is the code that checks theme settings
  // to see which icon version to use.
  $icon_style = '';

  // The setting is stored with the theme.
  if (($theme = variable_get('theme_default', 'nc_base_theme')) != NULL) {
    $icon_setting = theme_get_setting('document_type_icon_style', $theme);

    // For safety/sanitization
    switch ($icon_setting) {
      case 'alt':
        $icon_style = '-alt';
        break;

      case 'default':
      default:
        $icon_style = '';
        break;

    }
  }

  return $icon_style;
}

/**
 * Convert a filemime type to a CSS class.
 *
 * @param string $mimetype
 *   File mimetype.
 *
 * @return string
 *   Return a string representing a CSS class to be used to theme the file icon.
 */
function nc_fields_file_mime_to_doctype($mimetype) {
  // Initialize the doctype.
  $doctype = array(
    'icon' => '',
    'type' => '',
  );

  // This is the code that checks theme settings
  // to see which icon version to use.
  $icon_style = nc_fields_get_document_type_icon_style();

  // Convert mime type into a simplified string representation.
  $type = nc_fields_file_detect_class_from_mime_type($mimetype);

  // Update doctype based on simplified type representation.
  switch ($type) {
    case 'document':
      $doctype['icon'] = 'icon-doctype-doc' . $icon_style;
      $doctype['type'] = 'DOC';
      break;

    case 'pdf':
      $doctype['icon'] = 'icon-doctype-pdf' . $icon_style;
      $doctype['type'] = 'PDF';
      break;

    case 'image':
      $doctype['icon'] = 'icon-doctype-photo' . $icon_style;
      $doctype['type'] = 'IMAGE';
      break;

    case 'spreadsheet':
      $doctype['icon'] = 'icon-doctype-xls' . $icon_style;
      $doctype['type'] = 'XLS';
      break;

    case 'presentation':
      $doctype['icon'] = 'icon-doctype-ppt' . $icon_style;
      $doctype['type'] = 'PPT';
      break;

    case 'text':
    default:
      $doctype['icon'] = 'icon-doctype-text' . $icon_style;
      $doctype['type'] = 'TEXT';
      break;
  }

  return $doctype;
}

/**
 * Match the given MIME type to a given classification such as ...
 *
 *  - image
 *  - text
 *  - pdf
 *  - spreadsheet
 *  - presentation
 *  - document
 *  - default
 * .
 *
 * @param string $mimetype
 *   MIME type.
 *
 * @return string
 *   Return a simplified type (classification) of file
 */
function nc_fields_file_detect_class_from_mime_type($mimetype) {
  // The mimeMap is an array of key value pairs
  // The keys represent return values (i.e. simplified classes)
  // The values are an array of text patterns (for use with preg_match),
  // which can be used to map MIME to class.
  $mime_map = array(

    // Matches images.
    'image' => array(
      '/^image\/(.)*/',
    ),

    // Matches text files.
    'text' => array(
      '/^text\/(.)*/',
    ),

    // Matches PDFs (.pdf).
    'pdf' => array(
      '/(.)*\/(.)*(pdf)+(.)*/',
    ),

    // Matches spreadsheets (Excel .xls, .xlxs, etc).
    'spreadsheet' => array(
      '/^application\/(.)*excel(.)*/',
      '/^application\/vnd.openxmlformats-officedocument.spreadsheetml(.)*/',
    ),

    // Matches presentations (Powerpoint .ppt, .pptx, etc).
    'presentation' => array(
      '/^application\/vnd.ms-powerpoint(.)*/',
      '/^application\/vnd.openxmlformats-officedocument.presentationml(.)*/',
    ),

    // Matches documents (Word docs like .doc, .docx, etc).
    'document' => array(
      '/^application\/msword*/',
      '/^application\/vnd.ms-word(.)*/',
      '/^application\/vnd.openxmlformats-officedocument.wordprocessingml(.)*/',
    ),

    // This is here to catch anything not matched by the previous patterns.
    'default' => array(
      '/(.)*/',
    ),
  );

  // Search through the defined patterns, and return any matches.
  foreach ($mime_map as $key => $patterns) {
    foreach ($patterns as $pattern) {
      if (preg_match($pattern, $mimetype) === 1) {
        return $key;
      }
    }
  }

  // Ohgodhowdidthisgethereiamnotgoodwithcomputer.
  // Seriously, you shouldn't get here, but this is for safety.
  return 'default';
}

/**
 * Implements hook_preprocess_field().
 */
function nc_fields_preprocess_field(&$variables) {
  switch ($variables['element']['#formatter']) {
    case 'nc_fields_date_formatter':
      $variables['classes_array'][] = 'nc-date-field';
      break;

    case 'nc_fields_file_formatter_detail':
      $variables['classes_array'][] = 'nc-file-field';
      break;

    case 'nc_fields_term_formatter_simple':
      $variables['classes_array'][] = 'nc-term-field';
      break;
  }
}

